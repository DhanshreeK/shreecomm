<div class="panel panel-primary">
            <div class="panel-heading">Employee Details
        </div>
 <div class="panel-body">
<p id="notice"><%= notice %></p>
<%= link_to 'New Employee', new_employee_path, class: 'btn red' %>
     <div class="graph">
                              <div class="tables">
                            
                                <table class="table table-bordered" style="overflow-y: auto;height: 300px;width:100%">
                <thead>
    <tr>
     <th><b>Employee Number</b></th>
      <th><b>First name</b></th>
      <th><b>Middle name</b></th>
      <th><b>Last name</b></th>
      <th><b>Contact<b></th>
      <th><b>Email<b></th>
      <th><b>Address</b></th>
      <th><b>Employee type</b></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |employee| %>
      <tr>
      <td><%= employee.employee_number%></td>
        <td><%= employee.first_name %></td>
        <td><%= employee.middle_name %></td>
        <td><%= employee.last_name %></td>
        <td><%= employee.contact %></td>
        <td><%= employee.email %></td>
        <td><%= employee.address %></td>
        <td><%= employee.employee_type.name %></td>
        <td><%= link_to 'Show', employee %></td>
        <td><%= link_to 'Edit', edit_employee_path(employee)  %></td>
        <td><%= link_to 'Destroy', employee, method: :delete, data: { confirm: 'Are you sure?' }%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


</div></div></div>